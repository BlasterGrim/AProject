@model EFGetStarted.AspNetCore.NewDb.Models.Contatto

@{
    ViewData["Title"] = "Details";
}

<h2>Dettagli</h2>

<div>
    <h4>Contatto</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Telefono
        </dt>
        <dd>
            @Html.DisplayFor(model => model.nTelefono)
        </dd>
        <dt>
            FAX
        </dt>
        <dd>
            @Html.DisplayFor(model => model.nFax)
        </dd>
        <dt>
            Cellulare
        </dt>
        <dd>
            @Html.DisplayFor(model => model.nCellulare)
        </dd>
        <dt>
            E-mail
        </dt>
        <dd>
            @Html.DisplayFor(model => model.eMail)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.ContattoId">Modifica ></a> |
    <a asp-action="Index">Torna alla lista ></a>
</div>
